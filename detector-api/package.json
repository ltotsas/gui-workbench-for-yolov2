{
  "name": "detector-api",
  "version": "0.0.1",
  "description": "REST api for the dog breed detection",
  "main": "./build/index.js",
  "scripts": {
    "start": "node $npm_package_main",
    "start:w": "NODE_ENV=development nodemon --watch build -V -e js $npm_package_main",
    "build": "npm run compile && npm run lint && npm run copyFiles",
    "test": "mocha --reporter spec --compilers ts:ts-node/register 'test/**/*.test.ts'",
    "lint": "tslint --project tsconfig.json",
    "compile": "tsc",
    "compile:w": "tsc --watch",
    "copyFiles": "cpx \"src/protos/**\" build/protos/",
    "clean": "rm -rf .tscache/ build/ node_modules/",
    "prune:production": "npm prune --production && npm install --production",
    "build_and_prune": "npm run build && npm run prune:production"
  },
  "author": "Vitaly Bezgachev, vitaly.bezgachev@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/chai-http": "^3.0.4",
    "@types/debug": "0.0.30",
    "@types/del": "^3.0.1",
    "@types/express": "^4.16.0",
    "@types/lokijs": "^1.5.2",
    "@types/mocha": "^5.2.3",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.3.5",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cpx": "^1.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "csv-writer": "^1.2.0",
    "del": "^3.0.0",
    "express": "^4.17.3",
    "express-dynamic-static": "^1.0.5",
    "formidable": "^1.2.1",
    "grpc": "^1.12.4",
    "http-status": "^1.2.0",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.1.0",
    "js-yaml": "^3.12.0",
    "jsonfile": "^5.0.0",
    "lokijs": "^1.5.5",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "reflect-metadata": "^0.1.12",
    "tree-kill": "^1.2.1"
  }
}
